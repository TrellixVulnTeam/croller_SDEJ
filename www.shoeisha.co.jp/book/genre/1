<script type="text/javascript">
function cb_preview(data) {
    for (var p in data) {
        var preview = data[p].preview;
        if (preview == 'partial') {
            var prev_url = data[p].preview_url;
            var isbn = p.replace('ISBN:','');

            $('#pv_'+isbn).html('<a href="/book/preview/'+isbn+'"><span class="label label-primary"><i class="glyphicon glyphicon-book"></i> 立ち読み</span></a>');
        }
    }
}

$(document).ready(function() {
    var isbnlist = jQuery.map($('.isbn'), function(n) { return 'ISBN:'+$(n).text().trim(); }).join(',');
    var src_url = 'https://books.google.com/books?jscmd=viewapi&callback=cb_preview&bibkeys='+isbnlist;
    var scr = document.createElement("script");
    scr.async = true;
    scr.src = src_url;
    var s0 = document.getElementsByTagName('script')[0];
    s0.parentNode.insertBefore(scr, s0);
});

</script>


